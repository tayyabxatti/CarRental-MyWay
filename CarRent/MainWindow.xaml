<Window x:Class="CarRent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarRent"
        xmlns:vw="clr-namespace:CarRent.View"
        xmlns:vw1="clr-namespace:CarRent.View.Renters"
        xmlns:vw2="clr-namespace:CarRent.View.Agreementss"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <StackPanel>
        <TabControl>
            <TabItem Header="Reservation">
                <vw:ReservationList />
            </TabItem>
            <TabItem Header="Vehicles">
                <vw:Vehicle/>
            </TabItem>
            <TabItem Header="Rental Agreement">
                <vw2:AgreementList/>
            </TabItem>
            <TabItem Header="Driver">
                <vw:DriverMenu/>
            </TabItem>
            <TabItem Header=" Renters">
                <vw1:RenterMenu/>
            </TabItem>
        </TabControl>
    </StackPanel>
</Window>
